          <div class="form-group text-left">
            <label class="control-label">Yearly Income</label>
            <div class="row">
              <div class="col-6">
                <input id="salary" name="salary" placeholder="$" class="form-control" required="true" value=""
                  type="number"/>
              </div>
            </div>
          </div>

          <div class="form-group text-left">
            <label class="control-label">Menstruation</label>
